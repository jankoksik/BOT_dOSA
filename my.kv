<Button>:
    font_size:40
    color: (200/255, 214/255, 229/255,1)
    background_color: (131/255, 149/255, 167/255,0.5)


<FloatLayouts>:
    canvas.before:
        Color:
            rgba: (87/255, 101/255, 116/255,1)
        Rectangle:
            pos: self.pos
            size: self.size
    Button:
        pos_hint: {"x":0.05, "top":0.95}
        size_hint: 0.9, 0.2
        text: "LOGO"

    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x": 0.05 , "top":0.70}
        text: "SYN"
        on_press: root.CickedOne()

    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x":0.2875, "top":0.70}
        text: "App2"
        on_press: root.CickedTwo()
    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x":0.525, "top":0.70}
        text: "App3"
        on_press: root.CickedThree()
    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x":0.750, "top":0.70}
        text: "App4"
        on_press: root.CickedFour()

<P1>:
    Spinner:
        id: spinner_id
        on_text: root.spinner_clicked(spinner_id.text)
        text: "None"
        values: []
        size_hint: 0.7, 0.15
        pos_hint: {"x":0.0, "top":1}
    Button:
        id: scan_button
        size_hint: 0.3, 0.15
        pos_hint: {"x":0.7, "top":1}
        text: "Scan"
        on_press: root.ClickedConf1()



