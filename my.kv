<Button>:
    font_size:40
    color: (200/255, 214/255, 229/255,1)
    background_color: (131/255, 149/255, 167/255,0.5)


<FloatLayouts>:
    canvas.before:
        Color:
            rgba: (87/255, 101/255, 116/255,1)
        Rectangle:
            pos: self.pos
            size: self.size
    Button:
        pos_hint: {"x":0.05, "top":0.95}
        size_hint: 0.9, 0.2
        text: "LOGO"

    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x": 0.05 , "top":0.70}
        text: "WIFI"
        on_press: root.CickedOne()

    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x":0.2875, "top":0.70}
        text: "XXE"
        on_press: root.CickedTwo()
    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x":0.525, "top":0.70}
        text: "SQLI"
        on_press: root.CickedThree()
    Button:
        size_hint: 0.2, 0.2
        pos_hint: {"x":0.750, "top":0.70}
        text: "App4"
        on_press: root.CickedFour()

<P1>:
    Label:
        id: Ip_label
        text: "[b]IP:[/b]"
        size_hint: (0.7, 0.15)
        markup: True
        pos_hint: {"x":0.0, "top":1}
    Spinner:
        id: spinner_id
        on_text: root.spinner_clicked(spinner_id.text)
        text: "None"
        values: []
        size_hint: 0.7, 0.15
        pos_hint: {"x":0.0, "top":.85}
    Button:
        id: scan_button
        size_hint: 0.3, 0.15
        pos_hint: {"x":0.7, "top":.85}
        text: "Scan"
        on_press: root.ClickedConf1()
    Label:
        id: Attack_label
        text: "[b]Attack:[/b]"
        size_hint: (0.7, 0.15)
        markup: True
        pos_hint: {"x":0.0, "top":.70}
    Spinner:
        id: spinner_id_atk
        on_text: root.spinner_clicked_atk(spinner_id_atk.text)
        text: "SYN flood"
        values: ["SYN flood", "ARP spoof", "Smurf" ]
        size_hint: 0.7, 0.15
        pos_hint: {"x":0.0, "top":.55}
    Button:
        id: button_atk
        size_hint: 0.3, 0.15
        pos_hint: {"x":0.7, "top":.55}
        disabled: True 
        text: "Attack"
        on_press: root.ClickedConf1_atk()
    Label:
        id: Satus_label
        text: "[b]Attack status: IDLE[/b]"
        size_hint: (0.7, 0.15)
        markup: True
        pos_hint: {"x":0.0, "top":.10}



